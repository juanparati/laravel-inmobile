{
    "name": "juanparati/inmobile",
    "description": "inMobile client for Laravel",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Juan Lago",
            "email": "juanparati@gmail.com"
        }
    ],
    "minimum-stability": "stable",
    "scripts": {
        "test": "./vendor/bin/phpunit"
    },
    "require": {
        "php": ">=8.1",
        "ext-json": "*",
        "illuminate/support": ">=10",
        "guzzlehttp/guzzle": "^7.8"
    },
    "require-dev": {
        "phpunit/phpunit": ">=9.5",
        "orchestra/testbench": ">=8.2",
        "laravel/pint": "^1.13"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Juanparati\\Inmobile\\Providers\\InmobileProvider"
            ]
        },
        "aliases": {
            "Kickbox": "Juanparati\\Inmobile\\Facades\\InmobileFacade"
        }
    },
    "autoload": {
        "psr-4": {
            "Juanparati\\Inmobile\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Juanparati\\Inmobile\\Test\\": "test"
        }
    }
}
